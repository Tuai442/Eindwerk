<UserControl x:Class="FitnessCentra.PresentationWPF.Components.Tijdslot.TijdslotKiezer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FitnessCentra.PresentationWPF.Components.Tijdslot"
                     x:Name="root"

             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="200">

    <StackPanel Background="White">
        <Label x:Name="titel" Content="{Binding test, ElementName=root}"></Label>
        <Label FontSize="16">Kies een periode</Label>
        <WrapPanel x:Name="mainGrid">
            
        </WrapPanel>
        
        <StackPanel Orientation="Horizontal">
            <Button Width="100" Height="30" Content="Rezerveer" Click="Rezerveer_Click"/>
            <Button Width="100" Height="30" Content="Sluit" Click="Stop_Click"/>
        </StackPanel>
       
    </StackPanel>
    
</UserControl>
